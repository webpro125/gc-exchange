<h1>Consultants</h1>
<table id="consultant_lists">
  <thead>
  <td>Name</td>
  <td>Contact</td>
  <td>Status</td>
  </thead>
  <tbody>
  <% @consultants.each do |consultant| %>
      <tr>
        <td><%= link_to(consultant.full_name, consultant) %></td>
        <td><%= mail_to(consultant.email, 'Send Mail') %></td>
        <td><%= consultant.approved_status.label %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<%= paginate @consultants %>
