<h3><%= t('views.profile.headers.contact') %></h3>
<table class="contact">
  <%= render @consultant.phones %>
  <tr>
    <td>
      <%= Consultant.human_attribute_name(:email) %>
    </td>
    <td>
      <%= @consultant.email %>
    </td>
  </tr>
  <tr>
    <td>
      <%= Address.human_attribute_name(:address) %>
    </td>
    <% if @consultant.address.present? %>
      <td>
        <%= @consultant.address.address %>
      </td>
    <% end %>
  </tr>
</table>
